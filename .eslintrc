{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "node": true,
        "amd": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "impliedStrict": true
        }
    },

    "plugins": [
      "react"
    ],
    "extends":[
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "globals":{
      "alert": true,
      "fetch" : true,
      "window":true,
      "document":true
    },
    
    "rules":{
        "no-console":0
      }
}
